<!DOCTYPE html>
<html lang="pt-br">

<head>
  <meta charset="UTF-8">
  <meta http-equiv="X-UA-Compatible" content="IE=edge">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <link rel="stylesheet" href="/css/style.css">
  <link rel="shortcut icon" type="imagex/ico" href="/img/pokemon.ico">
  <title>Pokedex</title>
</head>

<body id="background-index">
  
    <nav id="nav">
      <audio id="a1" controls loop>
        <source src="/img/song.wav" type="audio/wav">
      </audio>
      <a class="c" id="c1" href="cadastro">Cadastro</a>
      <a class="d" id="d1" href="detalhes">Detalhes</a>
      <!--<section id="message">
              <p>Mensagem temp</p> <span id="close">&times;</span>
          </section>-->
    </nav>
  <header id="cabecalho">
    <h1>Pokedex</h1>
  </header>

  <section id="lista">
    <% pokedex.forEach(item=> { %>
      <section id="poke">
      <ul id="item.nome">
        <li>
          Nome: <%= item.Nome %>
        </li>
        <li>
          Tipo: <%= item.Tipo %>
        </li>
      </ul>

      <ul id="item.imagem">
        <li><img src="<%=item.Imagem%>" /></li>
      </ul>
    </section>

      <% }) %>
  </section>
</body>
<footer>
  <script src="/js/script.js"></script>
</footer>

</html>